{
    "name": "toolsets/revisionable",
    "description": "Easily store revisions of Laravel Eloquent Models",
    "keywords": ["revisionable", "revisions", "eloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Raf",
            "email": "raftalks@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Raftalks\\Revisionable\\": "src" }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/TestModel.php"
        ]
    },
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.5.*",
        "illuminate/database": "5.5.*",
        "illuminate/console": "5.5.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Raftalks\\Revisionable\\RevisionableServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "6.3.*"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
